                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": ":?query:",
                    "target": [
                        "TargetView1"
                    ]
                },
                {
                    "name": "StructureCreation",
                    "pattern": "StructureCreation",
                    "target": [
                        "TargetView2"
                    ]
                },
                {
                    "name": "ScratchCreate",
                    "pattern": "ScratchCreate",
                    "target": [
                        "TargetView3"
                    ]
                },
                {
                    "name": "LifingManagement",
                    "pattern": "LifingManagement",
                    "target": [
                        "TargetView6"
                    ]
                },
                {
                    "name": "BOMCreate",
                    "pattern": "BOMCreate",
                    "target": [
                        "TargetView4"
                    ]
                },
                {
                    "name": "SerialNumberCreation",
                    "pattern": "SerialNumberCreation",
                    "target": [
                        "TargetView5"
                    ]
                }
            ],
            "targets": {
                "TargetView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
