
<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
 
    <Dialog id="_IDGenDialog2" title = "{i18n>EDIT MATERIAL}">
    <l:Grid defaultSpan = "L12 M12 S12" width = "auto" id = "idGrid1">
    <l:content>
        <f:SimpleForm id="SimpleFormDisplay3541"
            minWidth="1024"
            maxContainerCols="2"
            editable="false"
            layout="ResponsiveGridLayout"
            labelSpanL="3"
            labelSpanM="3"
            emptySpanL="4"
            emptySpanM="4"
            columnsL="2"
            columnsM="2">
            <f:content>
                <Label id="_IDGenLabele" text="{i18n>Position}" />
                <Input id = "idLevele" enabled="false" value="{ematerial>/level}"/>
             
                <Label id="_IDGenLabelee" text="{i18n>Functional Group}" />
                <Input id = "idFgrpe" value="{ematerial>/fg}"/>
             
                <Label id="_IDGenLabel2e" text="{i18n>Material}" />
                <Input id = "idMateriale" value="{ematerial>/Material}"/> 

                <Label id="_IDGenLabel2sno" text="{i18n>Serial Number}" />
                <Input id = "idMaterialsno" value="{ematerial>/sno}"/>

                <Label id="_IDGenLabel2km" text="{i18n>Note}" />
                <Input id = "idMaterialkm" value="{ematerial>/km}"/>             
            </f:content>
        </f:SimpleForm>
    </l:content>
    </l:Grid>
    <buttons>
    <Button id="_IDGenButtone" text = "{i18n>CLOSE}" press = "onEditClose" type = "Reject"/>
    <Button id="_IDGenButton5e" text = "{i18n>SAVE}" press = "onEditSave" type = "Accept"/>
    </buttons>
    </Dialog>
</core:FragmentDefinition>